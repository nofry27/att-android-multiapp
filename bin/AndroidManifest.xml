<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multiapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="att.android.activity.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="att.android.activity.LoginActivity" >
        </activity>
        <activity android:name="att.android.activity.ContactListActivity" >
        </activity>
        <activity android:name="att.android.activity.RssActivity"></activity>
        <activity android:name="att.android.activity.WebViewActivity"></activity>
        <activity android:name="att.android.activity.ChatJointActivity"></activity>
        <activity android:name="att.android.activity.ChatTabActivity"></activity>
    </application>

</manifest>